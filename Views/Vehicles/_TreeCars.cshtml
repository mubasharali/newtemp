

<div class="ibox float-e-margins">
    <div class="ibox-title">
        <h5>Search By Brand or Model</h5>
        <div class="ibox-tools">
            <a class="collapse-link">
                <i class="fa fa-chevron-up"></i>
            </a>
        </div>
    </div>
    <div class="ibox-content">
        <input type="text" class="form-control" id="treeSearch" placeholder="search brand/model" />
        <div id="navigation">
            <ul data-bind="foreach:showCompanies">
                <li data-bind="visible:companyName">
                    <a data-bind="text:companyName"></a>
                    <ul data-bind="foreach:showModels">
                        <li data-bind="text:modelName"></li>
                    </ul>
                </li>
            </ul>
        </div>
    </div>
</div>



