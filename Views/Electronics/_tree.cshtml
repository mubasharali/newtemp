@{
    ViewBag.Title = "tree";
}

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-sm-3">
            <div class="ibox float-e-margins">
                <div class="ibox-title">
                    <h5>Categories</h5>
                    <div class="ibox-tools">
                        <a class="collapse-link">
                            <i class="fa fa-chevron-up"></i>
                        </a>
                        <a class="close-link">
                            <i class="fa fa-times"></i>
                        </a>
                    </div>
                </div>
                <div class="ibox-content">
                    <div id="navigation">
                        <ul>
                            <li>
                                <a href="#">Electronics</a>
                                <ul>
                                    <li>
                                        <a href="#">Mobies</a>
                                        <ul data-bind="foreach:showCompanies">
                                            <li>
                                                <a data-bind="text:companyName"></a>
                                                <ul data-bind="foreach:showModels">
                                                    <li data-bind="text:modelName"></li>
                                                </ul>
                                            </li>
                                        </ul>
                                    </li>
                                </ul>
                            </li>

                        </ul>
                        </div>
                    </div>
                    </div>
                </div>
            </div>
        </div>

